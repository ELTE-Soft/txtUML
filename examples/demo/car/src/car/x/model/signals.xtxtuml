package car.x.model;

datatype GearType {
	
	int gear;
	
	public GearType(int x) {
		gear = x;
	}
}

signal ChangeGear {  // shift the gear
	public GearType geary;	
};

datatype SpeedType {

	int speed;

	public SpeedType(int x) {
		speed = x;
	}

}

signal ChangeSpeed {
	public SpeedType speedy;
}

